pipeline {

    agent any

        stages {

            stage("build") {

                steps {
                    echo "building..."
                    sh "python --version"
                }

            }

            stage("test") {

                steps {
                    echo "testing..."
                }

            }

            stage("deploy") {

                steps {
                    echo "deploying..."
                }

            }

        }

    
}